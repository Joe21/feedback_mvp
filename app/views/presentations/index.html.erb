<div id='header'>
	<h1>Top 5 Feedback Responses</h1>
</div>

<div id="container">
	<% @all_feedbacks.each do |feedback| %>
	<h3><%= feedback.name %></h3><br />
	Description: <%= feedback.description %><br />
	<br />
	<img src="<%= feedback.image_url %>"/><br />
	<br />
	Video Demo: <a href="<%= feedback.media_link %>"><%= feedback.media_link %></a><br />
	<hr>
	<% end %>
</div>